{
  "type": "module",
  "name": "template-project",
  "version": "1.0.0",
  "description": "A template for a project : vite, typescript, react, react-router, tailwind, node, express, zustand, swr, eslint and prettier",
  "main": "index.js",
  "scripts": {
    "client": "npm --prefix client start",
    "client:dev": "npm --prefix client run dev",
    "client:build": "npm --prefix client run build",
    "client:preview": "npm --prefix client run preview",
    "server": "npm --prefix server start",
    "start": "npm run client:build && npm run server",
    "dev": "concurrently \"npm run client:dev\" \"npm run server\""
  },
  "keywords": [],
  "author": "Grego Nunez",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.0",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.3",
    "globals": "^15.6.0",
    "prettier": "3.3.2",
    "typescript-eslint": "^7.13.1"
  }
}
